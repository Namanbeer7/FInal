= Table.SelectRows(
    #"Reordered Columns",
    each (
        [Name] <> "High Level Summary" and 
        [Name] <> "Model Ticker Map" and
        (Text.Contains([Name], "Results") or Text.Contains([Name], "Additional Clients"))
    )
)

if Text.Contains([Name], "Additional Clients") then 
    Text.Start([Name], 7)  // Extracts "2Q 2025" from "2Q 2025 Additional Clients"
else 
    Text.Replace([Name], " Results", "")  // Removes " Results" from other sheets
